  <meta property="og:title" content="Johannes van der Merwe" />
  <meta
    property="og:description"
    content="Johannes van der Merwe is a technical problem-solver with a focus in Full Stack Development using ASP.NET & C#, SQL Server, React, Angular, and AWS."
  />
  <meta property="og:image" content="https://johannesmerwe.com/cv/img/j.png" />

  <!-- Meta for Twitter -->
  <meta name="twitter:card" content="summary_large_image" />
  <meta
    name="twitter:title"
    content="Johannes van der Merwe | Full Stack Developer"
  />
  <meta
    name="twitter:image"
    content="https://johannesmerwede.com/cv/img/j.png"
  />
  <meta
    name="twitter:description"
    content="Johannes van der Merwe is a technical problem-solver with a focus in Full Stack Development using ASP.NET & C#, SQL Server, React, Angular, and AWS."
  />

  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" href="./cv/img/favicon/favicon.ico" />

  <link
    rel="apple-touch-icon"
    sizes="180x180"
    href="./cv/img/favicon/apple-touch-icon.png"
  />
  <link
    rel="icon"
    type="image/png"
    sizes="32x32"
    href="./cv/img/favicon/favicon-32x32.png"
  />
  <link
    rel="icon"
    type="image/png"
    sizes="16x16"
