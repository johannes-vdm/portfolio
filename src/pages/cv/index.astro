  <link rel="preconnect" href="https://www.googletagmanager.com" />

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link
    href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,400;0,500&display=swap"
    rel="stylesheet"
  />

  <!-- Icons -->
  <link rel="dns-prefetch" href="https://cdn.jsdelivr.net" />
  <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css"
  />

  <!-- TODO Code Block Markdown Highlight -->
  <!-- <link id="light-highlight" rel="stylesheet"
      href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/11.7.0/styles/github.min.css">
